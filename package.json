{
  "name": "mark-xx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "dotenvx run -f .env -f .env.test --overload -- vitest run --coverage.enabled",
    "test:unit": "dotenvx run -f .env -f .env.test --overload -- vitest run --dir ./src/__tests__/unit/",
    "test:unit:watch": "dotenvx run -f .env -f .env.test --overload -- vitest --dir ./src/__tests__/unit/",
    "lint:check": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.39.0",
    "@phosphor-icons/react": "2.1.7",
    "@tailwindcss/vite": "4.1.1",
    "@tanstack/react-query": "5.71.3",
    "axios": "1.8.4",
    "dayjs": "1.11.13",
    "framer-motion": "12.6.3",
    "immer": "10.1.1",
    "lodash": "4.17.21",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-helmet-async": "2.0.5",
    "react-router-dom": "7.4.1",
    "react-type-animation": "3.2.0",
    "react-use": "17.6.0",
    "tailwind-merge": "3.2.0",
    "tailwindcss": "4.1.1",
    "zod": "3.24.2",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@types/lodash": "4.17.16",
    "@types/node": "22.13.17",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "@wooden-script/eslint-config": "1.1.3",
    "commitizen": "4.3.1",
    "concurrently": "9.1.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-vitest-globals": "1.5.0",
    "globals": "15.15.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "prettier-plugin-tailwindcss": "0.6.11",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.24.1",
    "vite": "6.2.0",
    "vitest": "3.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
