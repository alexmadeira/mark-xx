{
  "name": "mark-xx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "dotenvx run -f .env -f .env.test --overload -- vitest run --coverage.enabled",
    "test:unit": "dotenvx run -f .env -f .env.test --overload -- vitest run --dir ./src/__tests__/unit/",
    "test:unit:watch": "dotenvx run -f .env -f .env.test --overload -- vitest --dir ./src/__tests__/unit/",
    "lint:check": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc -b",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.45.2",
    "@radix-ui/react-slot": "1.2.3",
    "@tailwindcss/vite": "4.1.11",
    "@tanstack/react-query": "5.81.5",
    "@tsparticles/engine": "3.8.1",
    "@tsparticles/react": "3.0.0",
    "@tsparticles/slim": "3.8.1",
    "axios": "1.10.0",
    "colorizr": "3.0.8",
    "dayjs": "1.11.13",
    "immer": "10.1.1",
    "lenis": "1.3.4",
    "lodash": "4.17.21",
    "lucide-react": "0.525.0",
    "motion": "12.23.0",
    "potpack": "2.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-helmet-async": "2.0.5",
    "react-router-dom": "7.6.3",
    "react-use": "17.6.0",
    "react-wrap-balancer": "1.1.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.11",
    "zod": "3.25.67",
    "zustand": "5.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/bin-pack": "1.0.3",
    "@types/lodash": "4.17.20",
    "@types/node": "24.0.10",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "@wooden-script/eslint-config": "1.1.3",
    "commitizen": "4.3.1",
    "concurrently": "9.2.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-vitest-globals": "1.5.0",
    "globals": "16.2.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "prettier-plugin-tailwindcss": "0.6.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "7.0.0",
    "vitest": "3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "@tsparticles/engine",
      "esbuild"
    ]
  }
}
