{
  "name": "mark-xx",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm prismic:generate-types && vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "preview:build": "pnpm build && pnpm preview",
    "test": "dotenvx run -f .env -f .env.test --overload -- vitest run --coverage.enabled",
    "test:unit": "dotenvx run -f .env -f .env.test --overload -- vitest run --dir ./src/__tests__/unit/",
    "test:unit:watch": "dotenvx run -f .env -f .env.test --overload -- vitest --dir ./src/__tests__/unit/",
    "lint:check": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prismic:generate-types": "prismic-ts-codegen",
    "type-check": "tsc -b",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.49.0",
    "@prismicio/client": "7.21.2",
    "@radix-ui/react-portal": "1.1.10",
    "@radix-ui/react-slot": "1.2.4",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-query": "5.87.4",
    "@tanstack/react-query-persist-client": "5.87.4",
    "@tsparticles/engine": "3.9.1",
    "@tsparticles/react": "3.0.0",
    "@tsparticles/slim": "3.9.1",
    "axios": "1.11.0",
    "colorizr": "3.0.9",
    "copy-to-clipboard": "3.3.3",
    "dayjs": "1.11.19",
    "idb-keyval": "6.2.2",
    "immer": "10.1.3",
    "lenis": "1.3.17",
    "lodash": "4.17.21",
    "lottie-react": "2.4.1",
    "lucide-react": "0.543.0",
    "mitt": "3.0.1",
    "motion": "12.23.12",
    "mousetrap": "1.6.5",
    "mustache": "4.2.0",
    "potpack": "2.1.0",
    "react": "19.1.1",
    "react-countup": "6.5.3",
    "react-dom": "19.1.1",
    "react-helmet-async": "2.0.5",
    "react-router-dom": "7.8.2",
    "react-use": "17.6.0",
    "rxjs": "7.8.2",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.18",
    "use-sound": "5.0.0",
    "vite-plugin-package-version": "1.1.0",
    "zod": "4.1.5",
    "zustand": "5.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@prismicio/types": "0.2.9",
    "@types/lodash": "4.17.20",
    "@types/mousetrap": "1.6.15",
    "@types/mustache": "4.2.6",
    "@types/node": "25.0.3",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.2",
    "@wooden-script/eslint-config": "1.1.3",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-vitest-globals": "1.5.0",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.6",
    "prettier-plugin-tailwindcss": "0.6.14",
    "prismic-ts-codegen": "0.1.28",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.43.0",
    "vite": "7.1.5",
    "vite-plugin-pwa": "1.2.0",
    "vitest": "3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "@tsparticles/engine",
      "esbuild"
    ]
  }
}
